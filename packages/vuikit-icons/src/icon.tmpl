// icon-#{name}
export default {
  functional: true,
  props: {
    ratio: {
      type: [Number, String],
      default: 1
    }
  },
  render: function (h, { props, data }) {
    const { ratio } = props
    let {
      width = #{width},
      height = #{height},
      viewBox = '0 0 #{width} #{height}'
    } = (data.attrs || {})

    if (ratio !== 1) {
      width *= ratio
      height *= ratio
    }

    data.attrs = {
      version: '1.1',
      meta: 'vk-icons-#{name}',
      width,
      height,
      viewBox,
      ratio
    }

    data.domProps = {
      innerHTML: '#{data}'
    }

    return h('svg', data)
  }
}
