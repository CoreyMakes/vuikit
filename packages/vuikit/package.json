{
  "name": "vuikit",
  "version": "0.9.0",
  "description": "A responsive Vue UI library for web site interfaces based on UIkit",
  "license": "MIT",
  "author": {
    "name": "Miljan Aleksic",
    "url": "https://twitter.com/AleksicMiljan"
  },
  "homepage": "https://vuikit.js.org/",
  "bugs": "https://github.com/vuikit/vuikit/issues",
  "main": "dist/vuikit.cjs.js",
  "module": "dist/vuikit.esm.js",
  "keywords": [
    "library",
    "vue",
    "vuejs",
    "uikit",
    "vuikit"
  ],
  "files": [
    "dist/*.js",
    "lib/**/*.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve __dev__/main.js",
    "dev": "start-storybook -s ./.storybook/static -p 6006",
    "build": "node -r @std/esm scripts/build.js",
    "postbuild": "node -r @std/esm scripts/copyright.js",
    "test": "jest"
  },
  "dependencies": {
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@miljan/build": "^0.4.2",
    "@std/esm": "^0.26.0",
    "@storybook/vue": "^3.3.15",
    "@vue/cli-plugin-babel": "^3.0.0-beta.11",
    "@vue/cli-service": "^3.0.0-beta.11",
    "@vue/test-utils": "^1.0.0-beta.12",
    "date-fns": "^1.29.0",
    "globby": "^8.0.1",
    "jest": "^22.4.2",
    "jest-serializer-vue": "^1.0.0",
    "lodash": "^4.17.4",
    "replace-in-file": "^3.2.0",
    "rollup": "^0.58.2",
    "rollup-analyzer": "^1.1.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-cleanup": "^2.0.0",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.2.0",
    "rollup-plugin-replace": "^2.0.0",
    "style-loader": "^0.21.0",
    "uikit": "^3.0.0-beta.40",
    "vue-jest": "^2.1.1",
    "vue-loader": "^14.2.1",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.16",
    "vue-template-compiler": "^2.5.16"
  },
  "@std/esm": {
    "mode": "js",
    "cjs": true
  },
  "eslintConfig": {
    "globals": {
      "ENV": true
    },
    "env": {
      "jest": true
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "modulePaths": [
      "<rootDir>/../../node_modules",
      "<rootDir>/../../packages"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ]
  }
}
